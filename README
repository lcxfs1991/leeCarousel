leeCarousel

version: Beta 1


example:

<div id="carousal_wrap"></div>

<script type="text/javascript" src="leeCarousel.js"></script>

leeCarousel.init({

        id: "carousal_wrap",
        img: [
            "/Alibaba_Frontend/carousal/s1.png",
            "/Alibaba_Frontend/carousal/s2.png",
            "/Alibaba_Frontend/carousal/s3.png"
        ],
        style: "default",
        width: 800,
        height: 195,
        speed: 10
});